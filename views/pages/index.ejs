<!DOCTYPE html>
<html>
<head>
  <% include ../partials/header.ejs %>
</head>

<body>
  <div class="jumbotron text-center">
    <div class="container">
      <img src="/cbg.jpg">
    </div>
  </div>

  <div class="container">
    <div class="row">
      <table class="table table-condensed">
        <% articles.forEach(function(a) { %>
          <tr>
            <td><a href="<%=a.url%>" target="_blank"><%= a.title %></a></td>
            <td><%= a.date %></td>
          </tr>
        <% }) %>
      </table>
    </div>
  </div>

  <div class="container">
    <div class="row">
      <table class="table table-condensed">
        <% Object.keys(frequency).forEach(function(key) { %>
          <tr>
            <td><%= key %></td>
            <td><%= frequency[key] %></td>
          </tr>
        <% }) %>
      </table>
    </div>
  </div>

</body>
</html>
